{
    "compilerOptions": {
      "target": "ES2020",
      "module": "commonjs",
      "noImplicitAny": true,
      "sourceMap": true,
      "strict": true,
      "moduleResolution": "node",
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "lib": ["es2020","DOM"],
      "outDir": "pre-dist",
      "baseUrl": "main",
      "paths": {
        "@models/*": ["models/*"],
        "@utils/*": ["utils/*"],
        "@config/*":["config/*"]
      }
    },
    "exclude": [
        "node_modules",
        "dist",
        "renderer"
      ],
    "include": [
      "main/**/*"
    ]
  }